{
  "name": "dev-env",
  "scripts": {
    "mv:nvim": "rsync -avh --delete configs/nvim/ ~/.config/nvim",
    "mv:ranger": "rsync -avh --delete configs/ranger/ ~/.config/ranger",
    "mv:alacritty": "rsync -avh --delete configs/alacritty/ ~/.config/alacritty",
    "mv:hammerspoon": "rsync -avh --delete .hammerspoon/ ~/.hammerspoon",
    "mv:zshrc": "rsync -avh --delete .zshrc ~/.zshrc",
    "mv:tmux": "rsync -avh --delete .tmux.conf ~/.tmux.conf",
    "mv:zellij": "rsync -avh --delete configs/zellij/ ~/.config/zellij",
    "mv:debuggers": "rsync -avh --delete configs/debuggers/ ~/.config/debuggers",
    "mv:scripts": "rsync -avh --delete scripts/ ~/.dorage-scripts",
    "backup:karabiner": "rsync -avh --delete ~/.config/karabiner/ configs/karabiner"
  }
}
